ZSH_THEME="jonathan"
plugins=(
    git
    alias-finder
    aliases
    debian
    ubuntu
    sudo
    thefuck
    z
    # zsh-autosuggestions
    zsh-syntax-highlighting
    # zsh-autocomplete
)
zstyle ':autocomplete:*' min-delay 1

# remove ls highlight color
_ls_colors=":ow=01;33"
zstyle ':completion:*:default' list-colors "${(s.:.)_ls_colors}"
LS_COLORS+=$_ls_colors
